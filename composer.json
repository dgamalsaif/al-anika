{
  "name": "dgamalsaif/al-anika",
  "description": "Al Anika WordPress Theme",
  "type": "wordpress-theme",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dgamalsaif/al-anika.git"
    }
  ],
  "require": {
    "dgamalsaif/al-anika": "dev-main"
  },
  "extra": {
    "installer-paths": {
      "wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
